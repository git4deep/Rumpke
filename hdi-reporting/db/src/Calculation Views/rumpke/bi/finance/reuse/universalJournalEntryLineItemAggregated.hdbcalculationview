if(&quot;functionalAreaGroup&quot;='AMORTZTN',&quot;budget&quot;,0)-
if(&quot;functionalAreaGroup&quot;='INTEREST',&quot;budget&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualNI" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;actualNIBT&quot;-if(&quot;functionalAreaGroup&quot;='INCOME_TAX',&quot;actual&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budgetNI" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;budgetNIBT&quot;-if(&quot;functionalAreaGroup&quot;='INCOME_TAX',&quot;budget&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualEBITDA" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;actualNIBT&quot;+if(&quot;functionalAreaGroup&quot;='AMORTZTN',&quot;actual&quot;,0)+
if(&quot;functionalAreaGroup&quot;='CELL_AMRT',&quot;actual&quot;,0)+
if(&quot;functionalAreaGroup&quot;='DEPRECIATN',&quot;actual&quot;,0)+
if(&quot;functionalAreaGroup&quot;='INTEREST',&quot;actual&quot;,0)
</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budgetEBITDA" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
