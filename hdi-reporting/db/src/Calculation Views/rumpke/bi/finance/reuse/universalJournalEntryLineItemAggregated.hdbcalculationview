  <calculationView xsi:type="Calculation:JoinView" id="Join_14" cardinality="C1_1" joinType="leftOuter" optimizeJoinColumns="true">
    <descriptions/>
    <viewAttributes>
        <viewAttribute id="accountNumber"/>
        <viewAttribute id="costCenterName"/>
        <viewAttribute id="costCenter"/>
        <viewAttribute id="plant"/>
        <viewAttribute id="functionalArea"/>
        <viewAttribute id="contractType"/>
        <viewAttribute id="orderType"/>
        <viewAttribute id="distributionChannelPa"/>
        <viewAttribute id="versions"/>
        <viewAttribute id="lineOfBusiness"/>
        <viewAttribute id="fiscYearPer"/>
        <viewAttribute id="amountInCompanyCodeCurrency"/>
        <viewAttribute id="fiscalYear"/>
        <viewAttribute id="profitCenterText"/>
        <viewAttribute id="profitCenter"/>
        <viewAttribute id="distributionChannelName"/>
        <viewAttribute id="distributionChannel"/>
        <viewAttribute id="gLAccountLongText"/>
        <viewAttribute id="nameCompanyCode"/>
        <viewAttribute id="companyCode"/>
        <viewAttribute id="functionalAreaGroup"/>
        <viewAttribute id="functionalAreaGroupText"/>
        <viewAttribute id="area"/>
        <viewAttribute id="region"/>
        <viewAttribute id="district"/>
        <viewAttribute id="subDistrict"/>
        <viewAttribute id="contractTypeText"/>
        <viewAttribute id="orderTypeText"/>
        <viewAttribute id="nameofTheFunctionalArea"/>
        <viewAttribute id="postingPeriod"/>
        <viewAttribute id="quantity"/>
        <viewAttribute id="name"/>
        <viewAttribute id="distributionChannelPaText"/>
        <viewAttribute id="rumpkeMaterialGrpDescription"/>
        <viewAttribute id="rumpkeMaterialGrp"/>
        <viewAttribute id="areaDescription"/>
        <viewAttribute id="regionDescription"/>
        <viewAttribute id="districtDescription"/>
        <viewAttribute id="subDistrictDescription"/>
        <viewAttribute id="tradingPartnerCompanyID"/>
        <viewAttribute id="partnerProfitCenter"/>
        <viewAttribute id="accrualActuals"/>
        <viewAttribute id="employeeCostActuals"/>
        <viewAttribute id="fiscalYearPeriod"/>
      </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="DECIMAL" id="actual" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;versions&quot;='Actual' or &quot;versions&quot;='ACTUAL',&quot;amountinCCCurrencyFlipTRCOGSSign&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budget" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;versions&quot;='Budget' or &quot;versions&quot;='BUDGET',&quot;amountinCCCurrencyFlipTRCOGSSign&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="forecast" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;versions&quot;='Forecast' or &quot;versions&quot;='FORECAST',&quot;amountinCCCurrencyFlipTRCOGSSign&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="grossProfit" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','Gross Profit')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="costofOperations" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','Cost of Operations')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="operatingIncome" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','Operating Income')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="netIncomeBeforeTax" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','Net Income Before Tax')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="netIncome" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','Net Income')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="EBITDA" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','EBITDA')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="EBITDATARGET" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;functionalAreaGroup&quot;) or (&quot;functionalAreaGroup&quot;=' '),' ','EBITDA TARGET')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualGP" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;functionalAreaGroup&quot;='TOTAL_REV',&quot;actual&quot;,0)- 
if(&quot;functionalAreaGroup&quot;='COGS',&quot;actual&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budgetGP" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;functionalAreaGroup&quot;='TOTAL_REV',&quot;budget&quot;,0)- 
if(&quot;functionalAreaGroup&quot;='COGS',&quot;budget&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualCOOP" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;functionalAreaGroup&quot;='MAINTNCE',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='OPERATIONS',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='OTH_OPS',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='DEPRECIATN',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='TAXES',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='CORPALLOC',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='OPSALLOC',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='CELL_AMRT',&quot;actual&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='SGA',&quot;actual&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budgetCOOP" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>IF(&quot;functionalAreaGroup&quot;='MAINTNCE',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='OPERATIONS',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='OTH_OPS',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='DEPRECIATN',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='TAXES',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='CORPALLOC',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='OPSALLOC',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='CELL_AMRT',&quot;budget&quot;,0)+
IF(&quot;functionalAreaGroup&quot;='SGA',&quot;budget&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualOI" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;actualGP&quot;-&quot;actualCOOP&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budgetOI" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;budgetGP&quot;-&quot;budgetCOOP&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualNIBT" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;actualOI&quot;+if(&quot;functionalAreaGroup&quot;='OTHERINC',&quot;actual&quot;,0)-
if(&quot;functionalAreaGroup&quot;='AMORTZTN',&quot;actual&quot;,0)-
if(&quot;functionalAreaGroup&quot;='INTEREST',&quot;actual&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="budgetNIBT" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
