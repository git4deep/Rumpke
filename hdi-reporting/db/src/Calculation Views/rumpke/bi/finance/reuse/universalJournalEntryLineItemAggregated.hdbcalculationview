      <calculatedAttribute id="revenueCategory" hidden="false" order="46" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="Revenue Category"/>
        <keyCalculation datatype="NVARCHAR" expressionLanguage="COLUMN_ENGINE" length="40">
          <formula>(if(isNull (&quot;orderType&quot;),' ',&quot;orderType&quot;)+' '+
if(isNull (&quot;contractType&quot;),' ',&quot;contractType&quot;)+' '+
if(isNull (&quot;rumpkeMaterialGrp&quot;),' ',&quot;rumpkeMaterialGrp&quot;))</formula>
        </keyCalculation>
      </calculatedAttribute>
      <calculatedAttribute id="priorMonth" hidden="false" order="54" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="Prior Month - For Live Projection ONLY"/>
        <keyCalculation datatype="NVARCHAR" expressionLanguage="COLUMN_ENGINE" length="7">
          <formula>leftstr(string(addmonths(now(), -1)),4) + '0' + rightstr(leftstr(string(addmonths(now(), -1)),7),2)</formula>
        </keyCalculation>
      </calculatedAttribute>
