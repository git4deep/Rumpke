          <formula>&quot;fiscalYear&quot;+rightstr(&quot;postingPeriod&quot;,2)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="amountinCCCurrencyFlipTRCOGSSign" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if((&quot;versions&quot;='Actual' or &quot;versions&quot;='ACTUAL' or &quot;versions&quot;= 'Budget' or &quot;versions&quot;='BUDGET' or &quot;versions&quot;='Forecast' or &quot;versions&quot;='FORECAST') AND (&quot;functionalAreaGroup&quot; = 'TOTAL_REV' or &quot;functionalAreaGroup&quot; = 'OTHERINC' ),
&quot;amountInCompanyCodeCurrency&quot; * -1,&quot;amountInCompanyCodeCurrency&quot;) 



</formula>
        </calculatedViewAttribute>
