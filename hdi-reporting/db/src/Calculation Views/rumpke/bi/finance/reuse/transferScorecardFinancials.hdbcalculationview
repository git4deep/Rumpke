-
if((&quot;accountGroup&quot; = 'INCOME_STMT') AND &quot;functionalAreaGroup&quot;='MAINTNCE' AND IN(&quot;functionalArea&quot;,'RUFLET') AND (&quot;accountNumber&quot; = '0063020000'),&quot;actual&quot;,0))</formula>
      
        
        
        
        
