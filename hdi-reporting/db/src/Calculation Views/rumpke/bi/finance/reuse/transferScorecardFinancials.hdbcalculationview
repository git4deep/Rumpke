        <descriptions defaultDescription="Total Other Transfer Expenses - Actual"/>
        <formula>
((if((&quot;accountGroup&quot; = 'INCOME_STMT') AND &quot;functionalAreaGroup&quot;='MAINTNCE' AND IN(&quot;functionalArea&quot;,'RUFACT','RUFLET','YB99'),&quot;actual&quot;,0)
+
if((&quot;functionalAreaGroup&quot;='OPERATIONS') AND (&quot;functionalArea&quot;='RUOPS') AND (IN(&quot;subordinatesetID&quot;,'BUILDINGRENT','INTERCOLABOR','LEACHATE','MAINTENANCE','OTHER','PARTS','SITE_COSTS','SUPPLIES','UTILITIES','PROCESSINGFEES','PROSERVICES')),&quot;actual&quot;,0)
+
if((&quot;functionalAreaGroup&quot;='TAXES') AND (&quot;functionalArea&quot;='RTAX'),&quot;actual&quot;,0)
+
if(((&quot;functionalAreaGroup&quot;='OTH_OPS') AND IN(&quot;functionalArea&quot;,'RUSITE','RUSUPT') AND (&quot;accountGroup&quot; = 'INCOME_STMT')) ,&quot;actual&quot;,0)
