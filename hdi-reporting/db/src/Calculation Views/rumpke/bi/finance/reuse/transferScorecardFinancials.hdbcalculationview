        <viewAttribute id="profitCenterText"/>
        <viewAttribute id="region"/>
        <viewAttribute id="regionDescription"/>
        <viewAttribute id="subDistrict"/>
        <viewAttribute id="subDistrictDescription"/>
        <viewAttribute id="rumpkeMaterialGrp"/>
        <viewAttribute id="rumpkeMaterialGrpDescription"/>
        <viewAttribute id="calMonth"/>
        <viewAttribute id="calQuarter"/>
        <viewAttribute id="quantity"/>
        <viewAttribute id="forecast"/>
        <viewAttribute id="actual"/>
        <viewAttribute id="budget"/>
        <viewAttribute id="ebitdaActual"/>
        <viewAttribute id="ebitdaBudget"/>
        <viewAttribute id="grossProfitActual"/>
        <viewAttribute id="grossProfitBudget"/>
        <viewAttribute id="costOfOperationsActual"/>
        <viewAttribute id="costOfOperationsBudget"/>
        <viewAttribute id="operatingIncomeActual"/>
        <viewAttribute id="operatingIncomeBudget"/>
        <viewAttribute id="ebitdaTargetActual"/>
        <viewAttribute id="ebitdaTargetBudget"/>
        <viewAttribute id="netIncomeActual"/>
        <viewAttribute id="netIncomeBudget"/>
        <viewAttribute id="netIncomeBeforeTaxActual"/>
        <viewAttribute id="netIncomeBeforeTaxBudget"/>
        <viewAttribute id="accountGroup"/>
        <viewAttribute id="accountGroupDescription"/>
        <viewAttribute id="companyCode"/>
        <viewAttribute id="nameCompanyCode"/>
        <viewAttribute id="name"/>
        <viewAttribute id="gLAccountLongText"/>
        <viewAttribute id="distributionChannelPaText"/>
        <viewAttribute id="subordinatesetID"/>
        <viewAttribute id="ebitdaTargetForecast"/>
        <viewAttribute id="costOfOperationsForecast"/>
        <viewAttribute id="projection"/>
      </viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="fuelCostActual" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;subordinatesetID&quot; = 'FUEL') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS' OR &quot;functionalAreaGroup&quot; = 'MAINTNCE') AND (&quot;accountGroup&quot; = 'INCOME_STMT')) then &quot;actual&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="fuelCostBudget" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;subordinatesetID&quot; = 'FUEL') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS') AND (&quot;accountGroup&quot; = 'INCOME_STMT')) then &quot;budget&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="fuelCostForecast" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;subordinatesetID&quot; = 'FUEL') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS') AND (&quot;accountGroup&quot; = 'INCOME_STMT')) then &quot;forecast&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="equipmentRepairCostActual" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;functionalAreaGroup&quot; = 'MAINTNCE') AND (&quot;functionalArea&quot; = 'RUEQUP') AND (&quot;subordinatesetID&quot; != 'FUEL') AND (&quot;accountGroup&quot; = 'INCOME_STMT') AND (&quot;accountNumber&quot; != '0063104000')) then &quot;actual&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="equipmentRepairCostBudget" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;functionalAreaGroup&quot; = 'MAINTNCE') AND (&quot;functionalArea&quot; = 'RUEQUP') AND (&quot;subordinatesetID&quot; != 'FUEL') AND (&quot;accountGroup&quot; = 'INCOME_STMT') AND (&quot;accountNumber&quot; != '0063104000')) then &quot;budget&quot; else 0 end
</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="equipmentRepairCostForecast" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;functionalAreaGroup&quot; = 'MAINTNCE') AND (&quot;functionalArea&quot; = 'RUEQUP') AND (&quot;subordinatesetID&quot; != 'FUEL') AND (&quot;accountGroup&quot; = 'INCOME_STMT') AND (&quot;accountNumber&quot; != '0063104000')) then &quot;forecast&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="totalRevenueForecast" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when (&quot;functionalAreaGroup&quot; = 'TOTAL_REV' AND &quot;accountGroup&quot; = 'INCOME_STMT') then &quot;forecast&quot; else 0 end</formula>
      </calculatedViewAttribute>
    
      <calculatedViewAttribute id="totalRevenueActual" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
    
        <formula>case when (&quot;functionalAreaGroup&quot; = 'TOTAL_REV' AND &quot;accountGroup&quot; = 'INCOME_STMT') then &quot;actual&quot; else 0 end</formula>
    
      </calculatedViewAttribute>
    
      <calculatedViewAttribute id="totalRevenueBudget" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
    
        <formula>case when (&quot;functionalAreaGroup&quot; = 'TOTAL_REV' AND &quot;accountGroup&quot; = 'INCOME_STMT') then &quot;budget&quot; else 0 end</formula>
    
      </calculatedViewAttribute>
      <calculatedViewAttribute id="fuelCostProjection" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when ((&quot;subordinatesetID&quot; = 'FUEL') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS' OR &quot;functionalAreaGroup&quot; = 'MAINTNCE') AND (&quot;accountGroup&quot; = 'INCOME_STMT')) then &quot;projection&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="equipmentRepairCostProjection" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when (&quot;functionalAreaGroup&quot; = 'MAINTNCE' AND &quot;functionalArea&quot; = 'RUEQUP' AND &quot;subordinatesetID&quot; != 'FUEL' AND &quot;accountGroup&quot; = 'INCOME_STMT' AND &quot;accountNumber&quot; != '0063104000') then &quot;projection&quot; else 0 end</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="totalRevenueProjection" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>case when (&quot;functionalAreaGroup&quot; = 'TOTAL_REV' AND &quot;accountGroup&quot; = 'INCOME_STMT') then &quot;projection&quot; else 0 end</formula>
      </calculatedViewAttribute>
    
      <calculatedViewAttribute id="totalNetLaborCostForecast" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
    
        <formula>CASE WHEN ((&quot;subordinatesetID&quot; = 'DIROPSLABOR' OR &quot;subordinatesetID&quot; = 'INDOPSLAB' OR &quot;subordinatesetID&quot; = 'INDSGALAB' OR &quot;subordinatesetID&quot; = 'SUBCONTRACTING' OR &quot;subordinatesetID&quot; = 'EMPLOYEEEXPENSES' OR &quot;subordinatesetID&quot; = 'CELLCONS') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS')) 
THEN &quot;forecast&quot; ELSE 0 END</formula>
    
      </calculatedViewAttribute>
      <calculatedViewAttribute id="totalNetLaborCostActual" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>CASE WHEN ((&quot;subordinatesetID&quot; = 'DIROPSLABOR' OR &quot;subordinatesetID&quot; = 'INDOPSLAB' OR &quot;subordinatesetID&quot; = 'INDSGALAB' OR &quot;subordinatesetID&quot; = 'SUBCONTRACTING' OR &quot;subordinatesetID&quot; = 'EMPLOYEEEXPENSES' OR &quot;subordinatesetID&quot; = 'CELLCONS') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS')) 
THEN &quot;actual&quot; ELSE 0 END
</formula>
      </calculatedViewAttribute>
      <calculatedViewAttribute id="totalNetLaborCostBudget" datatype="DECIMAL" length="23" scale="2" expressionLanguage="SQL">
        <formula>CASE WHEN ((&quot;subordinatesetID&quot; = 'DIROPSLABOR' OR &quot;subordinatesetID&quot; = 'INDOPSLAB' OR &quot;subordinatesetID&quot; = 'INDSGALAB' OR &quot;subordinatesetID&quot; = 'SUBCONTRACTING' OR &quot;subordinatesetID&quot; = 'EMPLOYEEEXPENSES' OR &quot;subordinatesetID&quot; = 'CELLCONS') AND (&quot;functionalAreaGroup&quot; = 'OPERATIONS')) 
THEN &quot;budget&quot; ELSE 0 END</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
      <input node="Join_1">
        <mapping xsi:type="Calculation:AttributeMapping" target="accountGroup" source="setName"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="accountGroupDescription" source="shortDescriptionofaSet"/></input>
    </calculationView>
</calculationViews>
