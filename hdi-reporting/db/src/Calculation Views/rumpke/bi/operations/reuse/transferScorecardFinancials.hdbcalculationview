        <descriptions defaultDescription="All Other Costs - Actual"/>
        <formula>round(IF(in(&quot;functionalAreaGroup&quot;,'SG&amp;A','COGS','OPSALLOC','TAXES','CORPALLOC','AMORTZTN','INTEREST','OTHERINC',
'DEPRECIATN'),&quot;actual&quot;,0)  - (IF(&quot;accountGroup&quot; = 'LFTR-ALLOTHCOSTS', &quot;actual&quot;, 0)),2)</formula>
      </measure>
