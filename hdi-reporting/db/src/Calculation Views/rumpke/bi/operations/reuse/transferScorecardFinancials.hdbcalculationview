        <descriptions defaultDescription="Equipment Repair Cost - Actual"/>
        <formula>round(if( &quot;functionalAreaGroup&quot; != 'SG&amp;A' and in(&quot;accountNumber&quot;,'65007300', '65007200','63000400', '63000800','63000000','63000100','61043100','61023100','61033100','61103100','65006000', '63000600') OR &quot;accountNumber&quot; = '68888800' and in(&quot;accountGroup&quot;,'Maintenance','Intercompany Labor') AND &quot;functionalAreaGroup&quot; != 'SG&amp;A',&quot;actual&quot;,0),2)





</formula>
      </measure>
      <measure id="equipmentRepairCostBudget" hidden="false" order="54" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="Equipment Repair Cost - Budget"/>
        <formula>round(if( &quot;functionalAreaGroup&quot; != 'SG&amp;A' and in(&quot;accountNumber&quot;,'65007300', '65007200','63000400', '63000800','63000000','63000100','61043100','61023100','61033100','61103100','65006000', '63000600') OR &quot;accountNumber&quot; = '68888800' and in(&quot;accountGroup&quot;,'Maintenance','Intercompany Labor') AND &quot;functionalAreaGroup&quot; != 'SG&amp;A',&quot;budget&quot;,0),2)
