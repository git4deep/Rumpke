      <measure id="ebitdaTargetasPercentofTotalRevenueActual" hidden="false" order="62" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="EBITDA Target (% of Total Revenue) - Actual"/>
        <formula>If(&quot;totalRevenueActual&quot;&gt;0,&quot;ebitdaTargetActual&quot;/&quot;totalRevenueActual&quot;,0)</formula>
      </measure>
      <measure id="ebitdaTargetasPercentofTotalRevenueBudget" hidden="false" order="63" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
