      </measure>
      <measure id="caBenefitsRateProjection" hidden="false" order="173" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="4">
        <descriptions defaultDescription="Benefits Rate - Projection"/>
