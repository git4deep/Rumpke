      <measure id="caWageEligibleForBenefitsWeightedActual" hidden="false" order="165" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="Wage Eligible for Benefits Weighted - Actual"/>
        <formula>round(((&quot;rmWageEligibleForBenefitsActual&quot; * (1+&quot;caBenefitsRateActual&quot;)) + &quot;rmCommissionsActual&quot; + &quot;rmSubContractingActual&quot;),2)</formula>
      </measure>
      <measure id="caWageEligibleForBenefitsWeightedBudget" hidden="false" order="166" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="Wage Eligible for Benefits Weighted - Budget"/>
        <formula>round(((&quot;rmWageEligibleForBenefitsBudget&quot; * (1+&quot;caBenefitsRateBudget&quot;)) + &quot;rmCommissionsBudget&quot; + &quot;rmSubContractingBudget&quot;),2)</formula>
      </measure>
      <measure id="caWageEligibleForBenefitsWeightedForecast" hidden="false" order="167" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="Wage Eligible for Benefits Weighted - Forecast"/>
        <formula>round(((&quot;rmWageEligibleForBenefitsForecast&quot; * (1+&quot;caBenefitsRateForecast&quot;)) + &quot;rmCommissionsForecast&quot; + &quot;rmSubContractingForecast&quot;),2)</formula>
      </measure>
      <measure id="caWageEligibleForBenefitsWeightedProjection" hidden="false" order="168" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="Wage Eligible for Benefits Weighted - Projection"/>
        <formula>round(((&quot;rmWageEligibleForBenefitsProjection&quot; * (1+&quot;caBenefitsRateProjection&quot;)) + &quot;rmCommissionsProjection&quot; + &quot;rmSubContractingProjection&quot;),2)</formula>
      </measure>
      <measure id="caBenefitsRateActual" hidden="false" order="169" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="4">
        <descriptions defaultDescription="Benefits Rate - Actual"/>
        <formula>round(IF((&quot;rmDirectLaborActual&quot; + &quot;rmIndirectLaborActual&quot;) &gt; 0, &quot;rmEmployeeExpensesActual&quot; / (&quot;rmDirectLaborActual&quot; + &quot;rmIndirectLaborActual&quot;), 0),4)</formula>
      </measure>
      <measure id="caBenefitsRateBudget" hidden="false" order="170" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="4">
        <descriptions defaultDescription="Benefits Rate - Budget"/>
        <formula>round(IF((&quot;rmDirectLaborBudget&quot; + &quot;rmIndirectLaborBudget&quot;) &gt; 0, &quot;rmEmployeeExpensesBudget&quot; / (&quot;rmDirectLaborBudget&quot; + &quot;rmIndirectLaborBudget&quot;), 0),4)</formula>
      </measure>
      <measure id="caBenefitsRateForecast" hidden="false" order="171" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="4">
        <descriptions defaultDescription="Benefits Rate - Forecast"/>
