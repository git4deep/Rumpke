      <measure id="maintenanceCostperTruckHours" hidden="false" order="25" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="MAINT COST / HR"/>
        <formula> IF(ISNULL(round(IF(  &quot;truckHours&quot;&gt; 0,&quot;totalMaintenanceCost&quot;/&quot;truckHours&quot;,0),2)), 0 ,round(IF(  &quot;truckHours&quot;&gt; 0,&quot;totalMaintenanceCost&quot;/&quot;truckHours&quot;,0),2))</formula>
      </measure>
      <measure id="totalTruckCostperTruckHours" hidden="false" order="26" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
