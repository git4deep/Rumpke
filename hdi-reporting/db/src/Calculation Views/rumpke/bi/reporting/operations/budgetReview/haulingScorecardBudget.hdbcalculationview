        <formula>  IF(ISNULL(round(IF(  &quot;truckHours&quot;&gt; 0,&quot;totalTruckCost&quot;/&quot;truckHours&quot;,0),2)), 0 ,round(IF(  &quot;truckHours&quot;&gt; 0,&quot;totalTruckCost&quot;/&quot;truckHours&quot;,0),2))</formula>
      </measure>
      <measure id="routeHoursWithMechanicActualperLoads" hidden="false" order="27" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="HOURS / LOAD"/>
        <formula>round(IF( &quot;loads&quot; &gt; 0,&quot;routeHoursWithMechanic&quot;/&quot;loads&quot;,0),2)</formula>
      </measure>
      <measure id="yardsperRouteHoursWithMechanic" hidden="false" order="28" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="YARDS / TOTAL HOURS w/MECH"/>
        <formula>round(IF(  &quot;routeHoursWithMechanic&quot;&gt; 0,&quot;yards&quot;/&quot;routeHoursWithMechanic&quot;,0),2)</formula>
      </measure>
      <measure id="totalMaintenanceCostperTotalRevenue" hidden="false" order="29" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="4">
        <descriptions defaultDescription="MAIN COST % REV"/>
        <formula>IF(ISNULL(round(IF(  &quot;totalRevenue&quot;&gt; 0,&quot;totalMaintenanceCost&quot;/&quot;totalRevenue&quot;,0),4)), 0 ,round(IF(  &quot;totalRevenue&quot;&gt; 0,&quot;totalMaintenanceCost&quot;/&quot;totalRevenue&quot;,0),4))</formula>
      </measure>
      <measure id="routeCustomerMilesperLoads" hidden="false" order="30" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="MILES / LOAD"/>
        <formula>round(IF(&quot;loads&quot;&gt; 0,&quot;routeCustomerMiles&quot;/&quot;loads&quot;,0),2) </formula>
      </measure>
      <measure id="tonnageperLoads" hidden="false" order="31" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="TONS / LOAD"/>
        <formula>round(IF(&quot;loads&quot;&gt; 0,&quot;tonnage&quot;/&quot;loads&quot;,0),2) </formula>
      </measure>
      <measure id="liftsperRouteCustomerMiles" hidden="false" order="32" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="2">
        <descriptions defaultDescription="LIFTS / MILES"/>
        <formula>round(IF(&quot;routeCustomerMiles&quot;&gt; 0,&quot;lifts&quot;/&quot;routeCustomerMiles&quot;,0),2) </formula>
