      <measure id="ebitdaTargetperTotalRevenue" hidden="false" order="34" aggregationType="sum" measureType="simple" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="23" scale="4">
        <descriptions defaultDescription="EBITDA TRGT % REV"/>
        <formula>IF(&quot;totalRevenue&quot; &gt; 0, round(&quot;ebitdaTarget&quot; / &quot;totalRevenue&quot;,4),0)</formula>
      </measure>
