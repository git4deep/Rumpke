    </baseMeasures>
    <calculatedMeasures>
      <measure id="totalDamageClaims" hidden="false" order="1" semanticType="amount" aggregationType="sum" measureType="amount" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="34">
        <descriptions defaultDescription="3-Year Damage Claims"/>
        <fixedCurrency>USD</fixedCurrency>
        <formula>&quot;damageClaimsYear1&quot;+&quot;damageClaimsYear2&quot;+&quot;damageClaimsYear3&quot;</formula>
      </measure>
      <measure id="damageClaimsPerTruckHour" hidden="false" order="2" semanticType="amount" aggregationType="sum" measureType="amount" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="10" scale="3">
        <descriptions defaultDescription="3-Year Damage Claims per Truck Hour"/>
        <fixedCurrency>USD</fixedCurrency>
        <formula> if(&quot;totalTruckHours&quot; != 0, &quot;totalDamageClaims&quot;/&quot;totalTruckHours&quot; , 0)</formula>
      </measure>
      <measure id="damageClaimsPerTruckHourGoal" hidden="false" order="3" semanticType="amount" aggregationType="sum" measureType="amount" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="10" scale="3">
        <descriptions defaultDescription="Damage Claims per Truck Hour Goal"/>
        <fixedCurrency>USD</fixedCurrency>
        <formula>&quot;damageClaimsPerTruckHour&quot; * 0.97</formula>
      </measure>
      <measure id="totalTruckHours" hidden="false" order="4" aggregationType="sum" measureType="simple" datatype="BIGINT" expressionLanguage="COLUMN_ENGINE">
        <descriptions defaultDescription="Total Truck Hours"/>
        <formula>&quot;truckHoursYear1&quot;+&quot;truckHoursYear2&quot;+&quot;truckHoursYear3&quot;</formula>
      </measure>
      <measure id="damageClaimsPerTruckHourActual" hidden="false" order="13" semanticType="amount" aggregationType="sum" measureType="amount" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="10" scale="3">
        <descriptions defaultDescription="Damage Claim per Truck Hour - Actual"/>
        <fixedCurrency>USD</fixedCurrency>
        <formula>&quot;damageClaimsActual&quot;/if(&quot;truckHoursActual&quot; = 0, 1, &quot;truckHoursActual&quot;)</formula>
      </measure>
      <measure id="goalVariance" hidden="false" order="14" semanticType="amount" aggregationType="sum" measureType="amount" datatype="DECIMAL" expressionLanguage="COLUMN_ENGINE" length="10" scale="3">
        <descriptions defaultDescription="Variance"/>
        <fixedCurrency>USD</fixedCurrency>
        <formula> &quot;damageClaimsPerTruckHourActual&quot; - &quot;damageClaimsPerTruckHourGoal&quot;</formula>
      </measure>
    </calculatedMeasures>
    <restrictedMeasures/>
    <localDimensions>
