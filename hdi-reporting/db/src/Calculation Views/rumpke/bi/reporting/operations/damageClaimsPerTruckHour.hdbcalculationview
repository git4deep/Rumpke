        <descriptions defaultDescription="Truck Hours - Year 1"/>
        <measureMapping columnObjectName="Join_9" columnName="truckHoursYear1"/>
      </measure>
      <measure id="truckHoursYear2" order="16" aggregationType="sum" measureType="simple">
        <descriptions defaultDescription="Truck Hours - Year 2"/>
        <measureMapping columnObjectName="Join_9" columnName="truckHoursYear2"/>
      </measure>
      <measure id="damageClaimsActual" order="17" semanticType="amount" aggregationType="sum" measureType="amount">
        <descriptions defaultDescription="Damage Claims"/>
        <fixedCurrency>USD</fixedCurrency>
        <measureMapping columnObjectName="Join_9" columnName="damageClaimsActual"/>
