    <baseMeasures>
      <measure id="damageClaimsYear3" order="5" semanticType="amount" aggregationType="sum" measureType="amount">
        <descriptions defaultDescription="Damage Claims - Year 3"/>
        <fixedCurrency>USD</fixedCurrency>
        <measureMapping columnObjectName="Join_9" columnName="damageClaimsYear3"/>
      </measure>
      <measure id="damageClaimsYear2" order="6" semanticType="amount" aggregationType="sum" measureType="amount">
        <descriptions defaultDescription="Damage Claims - Year 2"/>
        <fixedCurrency>USD</fixedCurrency>
        <measureMapping columnObjectName="Join_9" columnName="damageClaimsYear2"/>
      </measure>
      <measure id="damageClaimsYear1" order="7" semanticType="amount" aggregationType="sum" measureType="amount">
        <descriptions defaultDescription="Damage Claims - Year 1"/>
        <fixedCurrency>USD</fixedCurrency>
        <measureMapping columnObjectName="Join_9" columnName="damageClaimsYear1"/>
