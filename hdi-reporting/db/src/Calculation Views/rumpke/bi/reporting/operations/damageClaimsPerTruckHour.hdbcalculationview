        <descriptions defaultDescription="Sub District"/>
        <keyMapping columnObjectName="Join_9" columnName="subDistrictDescription"/>
      </attribute>
      <attribute id="districtDescription" order="10" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="District"/>
        <keyMapping columnObjectName="Join_9" columnName="districtDescription"/>
      </attribute>
      <attribute id="regionDescription" order="11" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="Region"/>
        <keyMapping columnObjectName="Join_9" columnName="regionDescription"/>
      </attribute>
      <attribute id="areaDescription" order="12" attributeHierarchyActive="false" displayAttribute="false">
        <descriptions defaultDescription="Area"/>
        <keyMapping columnObjectName="Join_9" columnName="areaDescription"/>
      </attribute>
