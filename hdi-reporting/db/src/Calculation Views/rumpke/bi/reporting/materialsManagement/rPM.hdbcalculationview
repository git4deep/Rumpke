        <mapping xsi:type="Calculation:AttributeMapping" target="numberofMaterialDocument" source="numberofMaterialDocument"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="purchaseOrderHistoryCategory" source="purchaseOrderHistoryCategory"/>
        <mapping xsi:type="Calculation:AttributeMapping" target="documentDateinDocument" source="documentDateinDocument"/>
