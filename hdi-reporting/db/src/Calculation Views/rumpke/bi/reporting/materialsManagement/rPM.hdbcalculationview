      <viewAttribute hidden="true" id="JOIN$materialDocumentMax$numberofMaterialDocument"/>
    </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="VARCHAR" id="returnReasonKey" length="30" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;documentType&quot;='WE',&quot;reasonforMovement&quot;,'0')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="rejectedReason" length="20" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;purchaseOrderHistoryCategory&quot;='E',&quot;reasonforMovement&quot;,'0')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="creditInvoiceNumber" length="15" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;purchaseOrderHistoryCategory&quot;='Q',&quot;numberofMaterialDocument&quot;,'0')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="accountingYear" length="4" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isnull(&quot;materialDocumentYear&quot;),'',&quot;materialDocumentYear&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="accountingPeriod" length="3" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isnull(&quot;documentDateinDocument&quot;),'','0'+midstr(string(&quot;documentDateinDocument&quot;),6,2)) </formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="totalAmountReceived" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;amountinDocumentCurrency&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="statusOrderforHistory" length="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if((&quot;CountofTypesEandQ&quot;&gt;=2 AND &quot;purchaseOrderHistoryCategory&quot;='Q'), '3',(
if((isnull(&quot;purchaseOrderHistoryCategory&quot;) or &quot;purchaseOrderHistoryCategory&quot; = ' '),'1', (if(&quot;purchaseOrderHistoryCategory&quot; = 'E', '2','0')))))</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="RET_PO_STATUS_2_3">
