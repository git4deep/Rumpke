    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="NVARCHAR" id="stockorNonStock" length="10" expressionLanguage="COLUMN_ENGINE">
          <formula>if((isnull(&quot;materialDescription&quot;) or &quot;materialDescription&quot;=''),'Non-Stock','Stock')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="count" length="15" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>1</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="quantityofStockReturnedParts" length="20" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;stockorNonStock&quot;='Stock',&quot;POQuantity&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="quantityofNonStockReturnedParts" length="20" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;stockorNonStock&quot;='Non-Stock',&quot;POQuantity&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="noOfClosedorCancelledRPMs" length="20" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;statusOrder&quot;='3',1,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="rejectedParts" length="20" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isnull(&quot;rejectionIndicatorN&quot;) or &quot;rejectionIndicatorN&quot;='',0,&quot;POQuantity&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="noofCores" length="10" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;returnReasonDescription&quot;='Core Returns', 1*&quot;POQuantity&quot;,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="NVARCHAR" id="latitude" length="20" expressionLanguage="COLUMN_ENGINE">
          <formula>case (&quot;region&quot;, 'BLUEGRASS','39.609034',
'CINCINNATI','39.110687',
'CLEVELAND','41.518015',
'COLUMBUS','39.967081',
'CORPORATE','39.264567',
'DAYTON','39.764285',
'GREENVILLE','40.104713',
'LOUISVILLE','40.838103',
'WAVERLY','39.127539')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="longitude" length="20" expressionLanguage="COLUMN_ENGINE">
          <formula>case (&quot;region&quot;, 'BLUEGRASS','-83.554771',
'CINCINNATI','-84.523244',
'CLEVELAND','-81.674969',
'COLUMBUS','-81.997017',
'CORPORATE','-84.604623',
'DAYTON','-84.187746',
'GREENVILLE','-84.635459',
'LOUISVILLE','-81.259598',
'WAVERLY','-82.984349')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="statusOrderDescription" length="40" expressionLanguage="COLUMN_ENGINE">
          <formula>case(&quot;statusOrder&quot;,'1','PO Not Received',
'2', 'Waiting for Credit Invoice',
'3', 'Closed - Complete')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="statusOrder" length="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isnull(&quot;statusOrderforHistory&quot;),&quot;statusOrderforHistoryN&quot;,&quot;statusOrderforHistory&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="VARCHAR" id="CancelledDate_CK" length="8" expressionLanguage="COLUMN_ENGINE">
          <formula>string(&quot;cancelledDate&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="TotalAmountReceviedN" length="23" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;statusOrder&quot;='1',0, if(&quot;PurchasingDocumentTypeN&quot;='NB' and &quot;returnItemsFlag&quot;='X',&quot;amountDocumentCurrentyNB&quot;,&quot;DebitCreditAmount&quot;))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="TotalCreditMemoAmount" length="34" scale="2" expressionLanguage="COLUMN_ENGINE">
          <formula>&quot;netPriceinPurchasingDocCurrency&quot;*&quot;POQuantity&quot;</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="noOfClosedorCancelledRPMs_N" length="20" scale="0" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;statusOrder&quot;='3',1,0)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="POQuantity" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;TotalAmountReceviedN&quot;&lt;0,&quot;purchaseOrderQuantity&quot;*-1,&quot;purchaseOrderQuantity&quot;*1)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="Join_7"/>
    <filter>(not isNull(&quot;PurchasingDocumentTypeN&quot;))</filter>
  </calculationView>
