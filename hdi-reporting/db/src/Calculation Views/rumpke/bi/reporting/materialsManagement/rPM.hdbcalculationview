        <calculatedViewAttribute datatype="INTEGER" id="countonTypeE" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;purchaseOrderHistoryCategory&quot; = 'E', '1','0')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="INTEGER" id="countonTypeQ" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;purchaseOrderHistoryCategory&quot; = 'Q', '1','0')</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="DebitCreditAmount" length="23" scale="3" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;debitcreditIndicator&quot;='S',&quot;amountinDocumentCurrency&quot;*-1,&quot;amountinDocumentCurrency&quot;)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
    <restrictedViewAttributes/>
      <input node="rumpke.bi.materialsManagement.base::pOHistoryDocumentData"/>
    <filter>(in (&quot;purchaseOrderHistoryCategory&quot;,'E','Q'))</filter>
