  <calculationView xsi:type="Calculation:JoinView" id="RET_PO_MVMT_DTLS" dynamic="false" joinType="leftOuter">
    <descriptions/>
    <viewAttributes>
        <viewAttribute id="documentType"/>
        <viewAttribute id="reasonforMovement"/>
        <viewAttribute id="movementType"/>
