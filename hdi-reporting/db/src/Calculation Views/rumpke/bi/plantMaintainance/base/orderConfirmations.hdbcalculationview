      </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="NVARCHAR" id="confirmationAction" length="1" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;confirmationAction_1&quot;) AND &quot;STZHL&quot;!='00000000','R',
if(isNull(&quot;confirmationAction_1&quot;) AND &quot;STZHL&quot;='00000000','M',
if(&quot;confirmationAction_1&quot;='C','C','')))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="actualWorkHoursSignFlip" length="7" scale="6" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;confirmationAction&quot;='R',-1*&quot;cc_ActualWorkHours&quot;,&quot;cc_ActualWorkHours&quot;)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="minutesBetween" length="7" scale="5" expressionLanguage="COLUMN_ENGINE">
          <formula>(secondsbetween(&quot;START_JOB_TIMESTAMP&quot;,&quot;END_JOB_TIMESTAMP&quot;)/60)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
