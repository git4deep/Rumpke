  <calculationView xsi:type="Calculation:ProjectionView" id="differentDays" filterExpressionLanguage="COLUMN_ENGINE">
    <descriptions/>
    <viewAttributes>
        <viewAttribute id="client"/>
        <viewAttribute id="objectID"/>
        <viewAttribute id="plant"/>
        <viewAttribute id="orderNumber"/>
        <viewAttribute id="confirmationNumber"/>
        <viewAttribute id="confirmationCreationDate"/>
        <viewAttribute id="confirmationCounter"/>
        <viewAttribute id="employeeID"/>
        <viewAttribute id="workStartDate"/>
        <viewAttribute id="indicatorDocumentWasReversed"/>
        <viewAttribute id="operationActivityNumber"/>
        <viewAttribute id="employeeNameSAP"/>
        <viewAttribute id="workCenter"/>
        <viewAttribute id="unitForWork"/>
        <viewAttribute id="actualWorkHours"/>
        <viewAttribute id="START_JOB_DATE"/>
        <viewAttribute id="END_JOB_DATE"/>
        <viewAttribute id="START_JOB_TIMESTAMP"/>
        <viewAttribute id="END_JOB_TIMESTAMP"/>
        <viewAttribute id="confirmationAction"/>
        <viewAttribute id="actualWorkHoursSignFlip"/>
        <viewAttribute id="cc_ActualWorkHours"/>
      </viewAttributes>
    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="TIMESTAMP" id="sameDayLastSecond" expressionLanguage="COLUMN_ENGINE">
          <formula>if(isNull(&quot;START_JOB_DATE&quot;),longdate('19900101'+'235959')
,longdate(format(&quot;START_JOB_DATE&quot;,'YYYYMMDD')+'235959'))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="TIMESTAMP" id="nextDayFirstSecond" expressionLanguage="COLUMN_ENGINE">
          <formula>addseconds(&quot;sameDayLastSecond&quot;,2)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="workedHoursSameDay_Calc" length="15" scale="6" expressionLanguage="COLUMN_ENGINE">
          <formula>decfloat(decfloat(((secondsbetween(&quot;START_JOB_TIMESTAMP&quot;,&quot;sameDayLastSecond&quot;)+1)/60))/60)</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="workedHoursNextDay_Calc" length="15" scale="6" expressionLanguage="COLUMN_ENGINE">
          <formula>decfloat(decfloat(((secondsbetween(&quot;nextDayFirstSecond&quot;,&quot;END_JOB_TIMESTAMP&quot;)+1)/60))/60)</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
      <input node="Join_3"/>
    <filter>(&quot;START_JOB_DATE&quot;!=&quot;END_JOB_DATE&quot; AND &quot;confirmationAction&quot;='C' )</filter>
  </calculationView>
