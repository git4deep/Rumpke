    <calculatedViewAttributes>
        <calculatedViewAttribute datatype="SECONDDATE" id="workStartTimestamp" expressionLanguage="COLUMN_ENGINE">
          <formula>seconddate(string(&quot;workStartDate&quot;) +' '+
leftstr(&quot;workStartTime&quot;,2)+':'+midstr(&quot;workStartTime&quot;,3,2)+':'+rightstr(&quot;workStartTime&quot;,2))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="SECONDDATE" id="workEndTimestamp" expressionLanguage="COLUMN_ENGINE">
          <formula>seconddate(string(&quot;WorkEndDate&quot;) +' '+
leftstr(&quot;workEndTime&quot;,2)+':'+midstr(&quot;workEndTime&quot;,3,2)+':'+rightstr(&quot;workEndTime&quot;,2))</formula>
        </calculatedViewAttribute>
        <calculatedViewAttribute datatype="DECIMAL" id="cc_ActualWorkHours_unitAdjusted" length="7" scale="6" expressionLanguage="COLUMN_ENGINE">
          <formula>if(&quot;unitForWork&quot;='MIN',decfloat(&quot;actualWorkHours&quot;/60),
if(&quot;unitForWork&quot;='H',decfloat(&quot;actualWorkHours&quot;),0))</formula>
        </calculatedViewAttribute>
      </calculatedViewAttributes>
