PROCEDURE "rumpke.bi.dp.s4hana.pro::rep_ACDOCP_load_sp" ( )
   LANGUAGE SQLSCRIPT
AS
BEGIN
DECLARE ACDOCP_REQTSN NVARCHAR(23) ;
DECLARE VT_ACDOCP_REQTSN NVARCHAR (23);

select Max("REQTSN") into ACDOCP_REQTSN from "ACDOCP";
select Max("REQTSN") into VT_ACDOCP_REQTSN from "VT_ACDOCP";
IF :VT_ACDOCP_REQTSN > :ACDOCP_REQTSN 
THEN
call "rumpke.bi.dp.s4hana.rep::REP_ACDOCP.START_REPLICATION";

ELSE	
END IF;
END 